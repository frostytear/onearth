<?xml version="1.0" encoding="UTF-8"?>
<EnvironmentConfiguration>
    <GetCapabilitiesLocation service="wmts">{cache_path}/wmts_endpoint/</GetCapabilitiesLocation>
    <GetCapabilitiesLocation service="twms">{cache_path}/twms_endpoint/.lib/</GetCapabilitiesLocation>
    <GetTileServiceLocation>{cache_path}/twms_endpoint/.lib/</GetTileServiceLocation>
    <CacheLocation service="wmts" basename="cache_all_wmts">{cache_path}</CacheLocation>
    <CacheLocation service="twms" basename="cache_all_twms">{cache_path}</CacheLocation>
    <StagingLocation service="wmts">{testfile_dir}/wmts/epsg3857/</StagingLocation>
    <StagingLocation service="twms">{testfile_dir}/twms/epsg3857/</StagingLocation>
    <ServiceURL service="wmts">http://localhost/reproject/test/wmts/</ServiceURL>
    <ServiceURL service="twms">http://localhost/reproject/test/twms/</ServiceURL>
    <LegendLocation>{testfile_dir}/legends/</LegendLocation>
    <LegendURL>http://localhost/legends/</LegendURL>
    <ColorMapLocation>{testfile_dir}/colormaps/</ColorMapLocation>
    <ColorMapURL>http://localhost/colormaps/</ColorMapURL>
    <ReprojectEndpoint service="wmts">/reproject/test/wmts</ReprojectEndpoint>
    <ReprojectEndpoint service="twms">/reproject/test/twms</ReprojectEndpoint>
    <ReprojectApacheConfigLocation service="wmts" basename="onearth-reproject-wmts">{cache_path}/</ReprojectApacheConfigLocation>
    <ReprojectApacheConfigLocation service="twms" basename="onearth-reproject-twms">{cache_path}/</ReprojectApacheConfigLocation>
    <ReprojectLayerConfigLocation service="wmts">{cache_path}/wmts_endpoint/</ReprojectLayerConfigLocation>
    <ReprojectLayerConfigLocation service="twms">{cache_path}/twms_endpoint/</ReprojectLayerConfigLocation>    
</EnvironmentConfiguration>
